                         function aliIcmsVarConsFinal() {
                             var data = google.visualization.arrayToDataTable([
                                        ['Aliquota', 'Valores'],
                                        ['Maiores', @Html.Raw(ViewBag.AlqICMSVarejoCFMaior)],
                                        ['Menores', @Html.Raw(ViewBag.AlqICMSVarejoCFMenor)],
                                        ['Iguais', @Html.Raw(ViewBag.AlqICMSVarejoCFIgual)],
                                        ['Nulas MTX', @Html.Raw(ViewBag.AlqICMSVarejoCFNullaInterno)],
                                        ['Nulas Cliente', @Html.Raw(ViewBag.AlqICMSVarejoCFNullaExterno)]

                                    ]);

                                    var options = {
                                        title: 'Relação Cliente vs MTX',
                                        sliceVisibilityThreshold: .00001,
                                        pieHole: 0.3,
                                        pieSliceTextStyle: {
                                        color: 'black',
                                            fontSize: '10'
                                        },
                                        legend: 'true'
                                    };

                             var chart = new google.visualization.PieChart(document.getElementById('aliIcmsVarConsFinal'));
                            chart.draw(data, options);
                         }//fim venda varejo cons final

